    <sql id="selectLtWaybillVo">
        select id, waybill_no, shipper_id, dispatcher_id, driver_id, vehicle_id, cargo_info, weight, volume, sender_name, sender_phone, sender_address, receiver_name, receiver_phone, receiver_address, expected_time, pickup_time, finish_time, status, fee, paid, pay_time, receipt_path, create_time, update_time from LT_waybill
