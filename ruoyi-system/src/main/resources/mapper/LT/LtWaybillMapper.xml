    <select id="selectLtWaybillList" parameterType="LtWaybill" resultMap="LtWaybillResult">
        <include refid="selectLtWaybillVo"/>
        <where>  
            <if test="waybillNo != null  and waybillNo != ''"> and waybill_no = #{waybillNo}</if>
            <if test="shipperId != null "> and shipper_id = #{shipperId}</if>
